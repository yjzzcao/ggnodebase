    "url": "git+https://github.com/tlindig/position-calculator.git"
  },
  "keywords": [
    "position",
    "boundary",
    "placement",
    "calculate",
    "reference",
    "target",
    "viewport",
    "collision",
    "offset",
    "relative",
    "translate",
    "sticky",
    "flip",
    "fit",
    "ui"
  ],
